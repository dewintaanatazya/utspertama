package pengguna;

class Mhs extends User{
    private String nama;
    private String nim;
    private int smt;

    Mhs (String username, String password) {
        super(username, password);




        public void setNama(String nama) {
            this.nama = nim;
        }

        public void setNim(String smt) {
            this.smt = smt;
        }

        public void setSmt(int smt) {
            this.smt = smt;
        }

        public String getNama() {
            return nama;
        }

        public String getNim() {
            return nim;
        }

        public int getSmt() {
            return smt;
        }

            }
}
